; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Futsal Manager"
config/version="0.2.4"
run/main_scene="res://src/screens/splash/splash_screen.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/image="res://icon.png"
config/icon="res://icon.png"

[autoload]

Global="*res://src/global.gd"
ResUtil="*res://src/utils/res_util.gd"
MatchCombinationUtil="*res://src/utils/match_combination_util.gd"
TransferUtil="*res://src/utils/transfer_util.gd"
EmailUtil="*res://src/utils/email_util.gd"
Const="*res://src/const.gd"
FormatUtil="*res://src/utils/format_util.gd"
UiUtil="*res://src/utils/ui_util.gd"
IdUtil="*res://src/utils/id_util.gd"
RngUtil="*res://src/utils/rng_util.gd"
ThemeUtil="*res://src/utils/theme_util.gd"
LoadingUtil="*res://src/utils/loading_util.gd"
ThreadUtil="*res://src/utils/thread_util.gd"

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/unassigned_variable=2
gdscript/warnings/unassigned_variable_op_assign=2
gdscript/warnings/unused_variable=2
gdscript/warnings/unused_local_constant=2
gdscript/warnings/unused_private_class_variable=2
gdscript/warnings/unused_parameter=2
gdscript/warnings/unused_signal=2
gdscript/warnings/shadowed_variable=2
gdscript/warnings/shadowed_variable_base_class=2
gdscript/warnings/shadowed_global_identifier=2
gdscript/warnings/unreachable_code=2
gdscript/warnings/unreachable_pattern=2
gdscript/warnings/untyped_declaration=2
gdscript/warnings/unsafe_property_access=2
gdscript/warnings/unsafe_method_access=2
gdscript/warnings/property_used_as_function=2
gdscript/warnings/constant_used_as_function=2
gdscript/warnings/function_used_as_property=2

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/size/width=1920
window/size/height=1080

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

theme/custom="res://themes/default/theme.tres"

[internationalization]

locale/translations=PackedStringArray("res://assets/i18n/translations.de.translation", "res://assets/i18n/translations.en.translation", "res://assets/i18n/translations.it.translation")

[locale]

translations=PackedStringArray("res://assets/i18n/FutsalManagerI18N - Sheet1.de.translation", "res://assets/i18n/FutsalManagerI18N - Sheet1.en.translation", "res://assets/i18n/FutsalManagerI18N - Sheet1.it.translation")

[physics]

2d/default_gravity_vector=Vector2(0, 0)

[rendering]

textures/vram_compression/import_etc2_astc=true
