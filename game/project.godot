; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "ActionUtil",
"language": "GDScript",
"path": "res://src/match-simulator/action-util/ActionUtil.gd"
} ]
_global_script_class_icons={
"ActionUtil": ""
}

[application]

config/name="Futsal Manager"
run/main_scene="res://src/screens/splash/SplashScreen.tscn"
boot_splash/image="res://icon.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

DataSaver="*res://src/DataSaver.gd"
CalendarUtil="*res://src/utils/CalendarUtil.gd"
Leagues="*res://src/data/legaues/Leagues.gd"
MatchMaker="*res://src/utils/MatchMaker.gd"
TransferUtil="*res://src/utils/TransferUtil.gd"
EmailUtil="*res://src/utils/EmailUtil.gd"
ContractUtil="*res://src/utils/ContractUtil.gd"
Constants="*res://src/Constants.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[gui]

theme/custom="res://theme.tres"

[layer_names]

2d_physics/layer_1="ball"
2d_physics/layer_2="world"
2d_physics/layer_3="lines"
2d_physics/layer_4="player"
2d_physics/layer_5="field-spots"

[locale]

translations=PoolStringArray( "res://assets/i18n/FutsalManagerI18N - Sheet1.de.translation", "res://assets/i18n/FutsalManagerI18N - Sheet1.en.translation", "res://assets/i18n/FutsalManagerI18N - Sheet1.it.translation" )

[physics]

common/physics_fps=180
2d/default_gravity_vector=Vector2( 0, 0 )

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
